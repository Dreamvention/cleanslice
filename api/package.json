{
  "name": "cleanslice-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "docker": "docker compose up -d",
    "generate": "npx prisma-import --force",
    "dev": "npm run start:dev",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npx  prisma migrate deploy && node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "deploy": "nest build && npx cdktf deploy",
    "predev": "npm run docker && npm run migrate",
    "premigrate": "npx prisma-import --force",
    "migrate": "dotenv -e .env.dev -- npx prisma migrate dev && dotenv -e .env.dev -- npx prisma generate",
    "package": "node dist/slices/core/package.js"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.758.0",
    "@aws-sdk/client-lambda": "^3.758.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/client-sfn": "^3.758.0",
    "@aws-sdk/client-sns": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@codegenie/serverless-express": "^4.16.0",
    "@langchain/cohere": "^0.3.2",
    "@langchain/community": "^0.3.34",
    "@langchain/openai": "^0.4.4",
    "@nestjs-cognito/auth": "^2.1.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/devtools-integration": "^0.2.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/swagger": "^11.0.6",
    "@opensearch-project/opensearch": "^3.4.0",
    "@prisma/client": "^6.4.1",
    "@types/archiver": "^6.0.3",
    "@types/aws-lambda": "^8.10.147",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "ai": "^4.1.54",
    "archiver": "^7.0.1",
    "axios": "^1.8.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "constructs": "^10.4.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "dynamoose": "^4.0.3",
    "fs-extra": "^11.3.0",
    "globby": "^14.1.0",
    "gpt-3-encoder": "^1.1.4",
    "groq-sdk": "^0.15.0",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.19",
    "luxon": "^3.5.0",
    "multer": "^1.4.5-lts.1",
    "neo4j-driver": "^5.28.1",
    "node-html-markdown": "^1.3.0",
    "openai": "^4.86.2",
    "outdent": "^0.8.0",
    "redact-pii": "^3.4.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tokenizers": "^0.13.3",
    "ulid": "^2.3.0",
    "unstructured-client": "^0.21.0"
  },
  "devDependencies": {
    "@cdktf/provider-aws": "^19.56.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.11",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/jest": "29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "aws-sdk-client-mock": "^4.1.0",
    "cdktf": "^0.20.11",
    "construct": "^1.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "http-proxy-middleware": "^3.0.3",
    "jest": "29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.4.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "#node-web-compat": "./node-web-compat-node.js",
      "awslambda": "<rootDir>/test/__mocks__/awslambda.js",
      "^#/(.*)$": "<rootDir>/src/slices/$1",
      "^#(.*)$": "<rootDir>/src/slices/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  },
  "prisma": {
    "import": {
      "schemas": "./src/**/!(schema).prisma",
      "output": "./prisma/schema.prisma"
    }
  }
}
