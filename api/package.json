{
  "name": "cleanslice-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "docker": "docker compose up -d",
    "generate": "npx prisma-import --force",
    "dev": "npm run start:dev",
    "build": "nest build -p tsconfig.aws.json",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/lambda",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "deploy": "nest build -p tsconfig.aws.json && npx cdktf deploy",
    "predev": "npm run docker && npm run migrate",
    "premigrate": "npx prisma-import --force",
    "migrate": "dotenv -e .env.dev -- npx prisma migrate dev && dotenv -e .env.dev -- npx prisma generate",
    "package": "node dist/slices/core/package.js"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.521.0",
    "@aws-sdk/client-lambda": "^3.592.0",
    "@aws-sdk/client-s3": "^3.529.0",
    "@aws-sdk/client-sfn": "^3.632.0",
    "@aws-sdk/client-sns": "^3.738.0",
    "@aws-sdk/s3-request-presigner": "^3.564.0",
    "@codegenie/serverless-express": "^4.13.0",
    "@langchain/cohere": "^0.1.0",
    "@langchain/community": "^0.2.17",
    "@langchain/openai": "^0.2.1",
    "@nestjs-cognito/auth": "^1.1.7",
    "@nestjs/axios": "^3.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "^7.1.13",
    "@opensearch-project/opensearch": "^2.7.0",
    "@prisma/client": "^5.11.0",
    "@types/archiver": "^6.0.1",
    "@types/aws-lambda": "^8.10.137",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.11",
    "ai": "^2.2.37",
    "archiver": "^6.0.2",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "constructs": "^10.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.3.0",
    "dynamoose": "^4.0.0",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.0",
    "gpt-3-encoder": "^1.1.4",
    "groq-sdk": "^0.5.0",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.2.8",
    "luxon": "^3.4.4",
    "multer": "^1.4.5-lts.1",
    "neo4j-driver": "^5.22.0",
    "node-html-markdown": "^1.3.0",
    "openai": "^4.28.0",
    "outdent": "^0.8.0",
    "redact-pii": "^3.4.0",
    "redis": "^4.6.15",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "tokenizers": "^0.13.3",
    "ulid": "^2.3.0",
    "unstructured-client": "^0.10.6"
  },
  "devDependencies": {
    "@cdktf/provider-aws": "^19.18.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.4.3",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/jest": "29.5.1",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.16.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "aws-sdk-client-mock": "^4.1.0",
    "cdktf": "^0.20.7",
    "construct": "^1.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "http-proxy-middleware": "^3.0.0",
    "jest": "29.5.0",
    "prettier": "^3.4.2",
    "prisma": "^5.11.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "#node-web-compat": "./node-web-compat-node.js",
      "awslambda": "<rootDir>/test/__mocks__/awslambda.js",
      "^#/(.*)$": "<rootDir>/src/slices/$1",
      "^#(.*)$": "<rootDir>/src/slices/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  },
  "prisma": {
    "import": {
      "schemas": "./src/**/!(schema).prisma",
      "output": "./prisma/schema.prisma"
    }
  }
}
